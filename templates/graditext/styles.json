{
	"container": {
		"position": "relative",
		"width": "100%",
		"height": "100%",
		"maxHeight": "100%",
		"display": "flex",
		"flexDirection": "column",
		"alignItems": "center",
		"justifyContent": "center",
		"backgroundColor": "#000000"
	},
	"textBlock": {
		"display": "flex",
		"position": "absolute",
		"alignItems": "flex-start",
		"justifyContent": "center",
		"padding": "100px",
		"zIndex": 4,
		"lineHeight": 1,
		"width": "100%",
		"height": "100%",
		"flexDirection": "row",
		"top": 0,
		"left": 0,
		"color": "#ffffff",
		"fontFamily": "ProtokollBold"
	},
	"gradient": {
		"display": "flex",
		"position": "absolute",
		"top": "0px",
		"left": "0px",
		"zIndex": 1,
		"width": "100%",
		"height": "100%",
		"flexDirection": "row",
		"backgroundColor": "pink",
		"flexWrap": "wrap"
	},
	"image": {
		"position": "absolute",
		"top": 0,
		"left": 0,
		"zIndex": 3,
		"objectFit": "cover",
		"width": "100%",
		"height": "100%",
		"opacity": 0.4
	},
	"flex": {
		"display": "flex"
	}
}
