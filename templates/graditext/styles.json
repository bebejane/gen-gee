{
	"container": {
		"position": "relative",
		"width": "100%",
		"height": "100%",
		"maxHeight": "100%",
		"display": "flex",
		"flexDirection": "column",
		"alignItems": "center",
		"justifyContent": "center",
		"backgroundColor": "#000000"
	},
	"textBlock": {
		"display": "flex",
		"alignItems": "flex-start",
		"justifyContent": "center",
		"padding": "2rem",
		"zIndex": 2,
		"lineHeight": 1,
		"width": "100%",
		"height": "100%",
		"flexDirection": "column",
		"top": "0",
		"left": "0",
		"color": "#ffffff",
		"fontFamily": "ProtokollBold",
		"whiteSpace": "pre"
	},
	"gradient": {
		"display": "flex",
		"position": "absolute",
		"top": "0px",
		"left": "0px",
		"z-index": 1,
		"width": "100%",
		"height": "100%",
		"flexDirection": "row",
		"backgroundColor": "pink",
		"flexWrap": "wrap"
	},
	"flex": {
		"display": "flex"
	}
}
