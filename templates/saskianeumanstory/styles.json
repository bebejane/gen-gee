{
	"container": {
		"position": "relative",
		"width": "100%",
		"height": "100%",
		"display": "flex",
		"flexDirection": "column",
		"textAlign": "right",
		"alignItems": "flex-start",
		"justifyContent": "center",
		"backgroundColor": "#00000"
	},
	"image": {
		"position": "absolute",
		"top": "0",
		"left": "0",
		"zIndex": 1,
		"width": "100%",
		"height": "100%",
		"objectFit": "cover"
	},
	"text": {
		"display": "flex",
		"alignItems": "flex-start",
		"justifyContent": "center",
		"textAlign": "left",
		"padding": "6rem",
		"zIndex": 2,
		"width": "100%",
		"height": "33.333%",
		"flexDirection": "column",
		"color": "#ffffff",
		"fontFamily": "ProtokollBold",
		"fontSize": "100px"
	},
	"paragraph": {
		"display": "flex",
		"minHeight": "1em"
	},
	"italic": {
		"fontFamily": "ProtokollBold-Italic",
		"font=style": "italic"
	},
	"flex": {
		"display": "flex"
	}
}
