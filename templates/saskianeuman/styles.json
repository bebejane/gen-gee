{
	"container": {
		"position": "relative",
		"width": "100%",
		"height": "100%",
		"display": "flex",
		"flexDirection": "column",
		"textAlign": "right",
		"alignItems": "flex-end",
		"justifyContent": "center",
		"backgroundColor": "#00000"
	},
	"image": {
		"position": "absolute",
		"top": "0",
		"left": "0",
		"zIndex": "1",
		"width": "100%",
		"height": "100%",
		"minWidth": "100%",
		"minHeight": "100%",
		"objectFit": "cover"
	},
	"text": {
		"position": "absolute",
		"display": "flex",
		"alignItems": "flex-start",
		"justifyContent": "center",
		"padding": "10rem",
		"zIndex": "2",
		"width": "100%",
		"height": "100%",
		"flexDirection": "column",
		"top": "0",
		"left": "0",
		"color": "#ffffff",
		"fontFamily": "ProtokollBold",
		"fontSize": "100px"
	},
	"italic": {
		"fontFamily": "ProtokollBold-Italic",
		"fontStyle": "italic"
	},
	"flex": {
		"display": "flex"
	}
}
